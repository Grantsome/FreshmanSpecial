<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mredrock.freshmanspecial">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".APP"
        android:allowBackup="true"
        android:icon="@drawable/ic_face_white_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_face_white_24dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomepageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".strategy.activity.StrategyActivity">
        </activity>
        <activity android:name=".strategy.activity.ImageDetailActivity">
        </activity>
        <activity android:name=".data.activity.DataActivity">
        </activity>
        <activity android:name=".training.activity.TrainingActivity">
        </activity>
        <activity android:name=".training.activity.VideoWebViewActivity">
        </activity>
    </application>

</manifest>